{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "lldb",
      "request": "launch",
      "name": "Huyak",
      "program": "${workspaceFolder}/a.out",
      "args": [],
      "cwd": "${workspaceFolder}"
    },
    {
      "name": "Debug a.out",
      "type": "cppdbg",
      "MIMode": "gdb",
      "request": "launch",
      "program": "${workspaceFolder}/a.out",
      "args": [],
      "sourceFileMap": {
        "/path/to/main.c": "${workspaceFolder}/main.c",
        "/path/to/printf.s": "${workspaceFolder}/printf.s"
      },
      "stopAtEntry": true,
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "cppdbg",
      "request": "launch",
      "name": "andrews bebug",
      "program": "${workspaceFolder}/a.out",
      "args": [],
      "cwd": "${workspaceFolder}",
      "stopAtEntry": true,
      "breakpointMode": "file"
    },
    {
      "name": "C/C++: gcc сборка активного файла",
      "type": "cppdbg",
      "request": "launch",
      "program": "${fileDirname}/${fileBasenameNoExtension}",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${fileDirname}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "lldb",
      "preLaunchTask": "C/C++: gcc build active file"
    },
    {
      "name": "C/C++ Runner: Debug Session",
      "type": "cppdbg",
      "request": "launch",
      "args": [],
      "stopAtEntry": false,
      "externalConsole": false,
      "cwd": "/root/AsmPrintf",
      "program": "/root/AsmPrintf/build/Debug/outDebug",
      "MIMode": "gdb",
      "miDebuggerPath": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}